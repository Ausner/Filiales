<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>ServletLogin</servlet-name>
        <servlet-class>Servlets.ServletLogin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletLogin</servlet-name>
        <url-pattern>/principal</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ServletCondominio</servlet-name>
        <servlet-class>Servlets.ServletCondominio</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletCondominio</servlet-name>
        <url-pattern>/condominio</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ServletQueja</servlet-name>
        <servlet-class>Servlets.ServletQueja</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletQueja</servlet-name>
        <url-pattern>/queja</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ServletEditarQueja</servlet-name>
        <servlet-class>Servlets.ServletEditarQueja</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletEditarQueja</servlet-name>
        <url-pattern>/Editarqueja</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ServletCalcularMontos</servlet-name>
        <servlet-class>Servlets.ServletCalcularMontos</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletCalcularMontos</servlet-name>
        <url-pattern>/CalcularMontos</url-pattern>
    </servlet-mapping>
    
</web-app>